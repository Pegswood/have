func SortSlice[V](slice []V, cmp func(V, V) bool):
    for var i = 0; i < len(slice); i += 1:
        for var j = i + 1; j < len(slice); j += 1:
            if cmp(slice[i], slice[j]):
                slice[i], slice[j] = slice[j], slice[i]

